<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="container">
    <property name="height">400</property>
    <property name="width">500</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">nsew</property>
      <rows>
        <row id="0">
          <property name="pad">0</property>
          <property name="weight">0</property>
        </row>
        <row id="1">
          <property name="weight">1</property>
        </row>
      </rows>
      <columns>
        <column id="0">
          <property name="weight">1</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="ttk.Frame" id="topFrame">
        <property name="height">0</property>
        <property name="padding">5</property>
        <property name="width">500</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">new</property>
          <rows>
            <row id="0">
              <property name="pad">10</property>
              <property name="weight">1</property>
            </row>
            <row id="2">
              <property name="pad">0</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="pad">50</property>
              <property name="weight">0</property>
            </column>
            <column id="1">
              <property name="pad">50</property>
              <property name="weight">0</property>
            </column>
            <column id="2">
              <property name="pad">100</property>
              <property name="weight">0</property>
            </column>
            <column id="3">
              <property name="pad">100</property>
              <property name="weight">0</property>
            </column>
          </columns>
        </layout>
        <child>
          <object class="ttk.Label" id="Label_2">
            <property name="text" translatable="yes">Input(a/b):</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="Entry_2">
            <property name="font">TkDefaultFont</property>
            <property name="state">normal</property>
            <property name="takefocus">true</property>
            <property name="textvariable">string:inp</property>
            <property name="validate">none</property>
            <property name="width">8</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="Button_2">
            <property name="command">onClick</property>
            <property name="text" translatable="yes">Calculate</property>
            <layout>
              <property name="column">3</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="bottomFrame">
        <property name="height">400</property>
        <property name="width">500</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">nsew</property>
          <rows>
            <row id="0">
              <property name="weight">1</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="weight">1</property>
            </column>
          </columns>
        </layout>
        <child>
          <object class="ttk.Notebook" id="Notebook_1">
            <property name="height">400</property>
            <property name="width">500</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
            <child>
              <object class="ttk.Notebook.Tab" id="Tab_1">
                <property name="text" translatable="yes">Geodesic Paths</property>
                <child>
                  <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_2">
                    <property name="scrolltype">both</property>
                    <property name="usemousewheel">true</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                      <property name="sticky">nsew</property>
                      <rows>
                        <row id="0">
                          <property name="weight">1</property>
                        </row>
                      </rows>
                      <columns>
                        <column id="0">
                          <property name="weight">1</property>
                        </column>
                      </columns>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="Label_1">
                        <property name="font">{Cambria} 12 {}</property>
                        <property name="textvariable">string:outPaths</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="propagate">True</property>
                          <property name="row">0</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Notebook.Tab" id="Tab_2">
                <property name="text" translatable="yes">Integer Continued Fraction</property>
                <child>
                  <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_6">
                    <property name="scrolltype">both</property>
                    <property name="usemousewheel">true</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                      <property name="sticky">nsew</property>
                      <rows>
                        <row id="0">
                          <property name="weight">1</property>
                        </row>
                      </rows>
                      <columns>
                        <column id="0">
                          <property name="weight">1</property>
                        </column>
                      </columns>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="Label_6">
                        <property name="font">{Cambria} 12 {}</property>
                        <property name="textvariable">string:outICF</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="propagate">True</property>
                          <property name="row">0</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Notebook.Tab" id="Tab_3">
                <property name="text" translatable="yes">Block and New Block Forms</property>
                <child>
                  <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_7">
                    <property name="scrolltype">both</property>
                    <property name="usemousewheel">true</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                      <property name="sticky">nsew</property>
                      <rows>
                        <row id="0">
                          <property name="weight">1</property>
                        </row>
                      </rows>
                      <columns>
                        <column id="0">
                          <property name="weight">1</property>
                        </column>
                      </columns>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="Label_7">
                        <property name="font">{Cambria} 12 {}</property>
                        <property name="textvariable">string:outWord</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="propagate">True</property>
                          <property name="row">0</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Notebook.Tab" id="Tab_4">
                <property name="state">normal</property>
                <property name="text" translatable="yes">Matrix Multiplications</property>
                <child>
                  <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_8">
                    <property name="scrolltype">both</property>
                    <property name="usemousewheel">true</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                      <property name="sticky">nsew</property>
                      <rows>
                        <row id="0">
                          <property name="weight">1</property>
                        </row>
                      </rows>
                      <columns>
                        <column id="0">
                          <property name="weight">1</property>
                        </column>
                      </columns>
                    </layout>
                    <child>
                      <object class="ttk.Label" id="Label_8">
                        <property name="font">{Cambria} 12 {}</property>
                        <property name="textvariable">string:out</property>
                        <property name="underline">0</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="ipadx">10</property>
                          <property name="ipady">10</property>
                          <property name="propagate">True</property>
                          <property name="row">0</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
